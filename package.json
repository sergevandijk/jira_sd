    "private": true,
    "scripts": {
        "test": "yarn test:karma && yarn test:jest",
        "test:jest": "jest --silent",
        "test:karma": "NODE_ENV=karma-test karma start sd-components/frontend-plugin/build/tests/config/karma-conf.js",
        "test:karma:fast": "yarn test:karma --no-soy-compile",
        "test:integration": "NODE_ENV=integration mocha --opts .mocha.opts",
        "test:ci": "yarn test:karma --ci && yarn test:jest",
        "lint": "yarn lint:js && yarn lint:ts && yarn lint:styles && yarn lint:json",
        "lint:fix": "yarn lint:js:fix && yarn lint:ts:fix && yarn lint:styles:fix && yarn lint:json:fix",
        "lint:js": "eslint .",
        "lint:js:fix": "yarn lint:js --fix",
        "lint:ts": "tslint --project ./tsconfig.json --exclude **/frontend-legacy/**/*.js",
        "lint:ts:fix": "yarn run lint:ts --fix",
        "lint:styles": "stylelint '**/*.{css,less,scss}' && prettier -l '**/*.{css,less,scss}'",
        "lint:styles:fix": "stylelint '**/*.{css,less,scss}' --fix; prettier --write '**/*.{css,less,scss}'",
        "lint:json": "prettier -l '**/*.json'",
        "lint:json:fix": "prettier --write '**/*.json'",
        
        "mirror:update": "node pkg-scripts/update-offline-mirror.js",
        "build": "webpack --config sd-components/frontend-plugin/build/webpack.config.js --env production",
        "build-report": "webpack --config sd-components/frontend-plugin/build/webpack.config.js --env.target build-report",
        "build-report:ci": "webpack --config sd-components/frontend-plugin/build/webpack.config.js --env.target build-report-ci",
        "build:progress": "yarn build --progress",
        "dev": "webpack --config sd-components/frontend-plugin/build/webpack.config.js --env development",
        "analyze": "webpack --config sd-components/frontend-plugin/build/webpack.config.js --env analyze",
        "watch": "webpack-dev-server --config sd-components/frontend-plugin/build/webpack.config.js --env watch",
        "watch:translations": "node sd-components/frontend-plugin/build/generate-watch-translations.js",
        "watch:prepare": "webpack --config sd-components/frontend-plugin/build/webpack.config.js --env watch_prepare",
        "watch:explore": "ENABLE_EXPLORER=true ./sdmake watch",
        "analytics:validate": "node pkg-scripts/validate-analytics.js",
        "vr:update": "visual-regression-update --config .vis-reg-config.js",
        "vr:test": "visual-regression-test --config .vis-reg-config.js",
        "vr:report": "visual-regression-report --config .vis-reg-config.js",
        "stats": "webpack --config sd-components/frontend-plugin/build/webpack.config.js --env production --profile --json > stats-new.json",
        "storybook": "start-storybook -p 9001"
    },
    "workspaces": [
        "packages/*"
    ],
    "jest": {
        "reporters": [
            "default",
            [
                "jest-junit",
                {
                    "outputDirectory": "./tests/js-unit-tests-parent/js-unit-tests/target",
                    "outputName": "./jest-junit.xml"
                }
            ]
        ],
        "transform": {
            "^.+\\.tsx?$": "ts-jest",
            "^.+\\.js$": "babel-jest",
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/sd-components/frontend-plugin/build/jest/file-mock-transform.js"
        },
        "testRegex": "sd-components/frontend-plugin/src/main/frontend/.*\\.test\\.tsx?$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "json"
        ],
        "moduleNameMapper": {
            "axios": "<rootDir>/sd-components/frontend-plugin/build/tests/mocks/axios",
            "core-ui/utils/help-links": "<rootDir>/sd-components/frontend-plugin/build/tests/mocks/help-links-mock.js",
            "core-ui/utils/tracking": "<rootDir>/sd-components/frontend-plugin/build/tests/mocks/utils-tracking-mock.js",
            "frontend/(.*)": "<rootDir>/sd-components/frontend-plugin/src/main/frontend/$1",
            "jira/util/formatter": "<rootDir>/sd-components/frontend-plugin/build/tests/mocks/jira-formatter-mock.js",
            "jira/flag": "<rootDir>/sd-components/frontend-plugin/build/tests/mocks/jira/flag.js",
            "servicedesk/moment": "moment",
            "servicedesk/underscore": "underscore",
            "servicedesk/jQuery": "jquery",
            "wrm/context-path": "<rootDir>/sd-components/frontend-plugin/build/tests/mocks/wrm/context-path.js",
            "wrm/require": "<rootDir>/sd-components/frontend-plugin/build/tests/mocks/wrm/require.js",
            "\\.(css|less)$": "identity-obj-proxy",
            "^wr-dependency\\!(.*)$": "<rootDir>/sd-components/frontend-plugin/build/jest/wrm-dependency-stub.js"
        },
        "modulePaths": [
            "<rootDir>/sd-components/frontend-plugin/src/main/frontend",
            "<rootDir>/sd-components/frontend-plugin/src/main/frontend-legacy"
        ],
        "setupTestFrameworkScriptFile": "<rootDir>/sd-components/frontend-plugin/build/jest/setup-jest.js",
        "collectCoverageFrom": [
            "<rootDir>/sd-components/frontend-plugin/src/main/frontend/**/*.{js,ts,tsx}",
            "!<rootDir>/sd-components/frontend-plugin/src/main/frontend/**/*.stories.{js,ts,tsx}",
            "!<rootDir>/sd-components/frontend-plugin/src/main/frontend/**/*.d.ts"
        ],
        "snapshotSerializers": [
            "enzyme-to-json/serializer"
        ],
        "transformIgnorePatterns": [
            "<rootDir>/node_modules/(?!(@atlaskit/.*))"
        ]
    },
    "loki": {
        "configurations": {
            "chrome.laptop": {
                "target": "chrome.docker",
                "width": 1366,
                "height": 768,
                "deviceScaleFactor": 1,
                "mobile": false,
                "fitWindow": false
            }
        }
    }
}
